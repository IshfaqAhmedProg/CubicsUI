{
  "name": "@cubicsui/db",
  "version": "1.1.1",
  "description": "The database that holds all the constants required to generate components",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": "./dist/main.js",
  "scripts": {
    "lint": "eslint",
    "build": "pnpm lint && tsc && pnpm db:generate",
    "clean": "rimraf node_modules .turbo dist",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "postinstall": "prisma generate"
  },
  "keywords": [
    "db",
    "studiocubics",
    "cubicsui",
    "cui"
  ],
  "author": {
    "name": "Studio Cubics",
    "email": "studiocubics7@gmail.com",
    "url": "https://studio-cubics.vercel.app"
  },
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.2.1",
    "prisma": "^6.2.1",
    "case": "^1.6.3"
  },
  "devDependencies": {
    "@cubicsui/configs": "workspace:^",
    "typescript": "^5.7.2"
  }
}
