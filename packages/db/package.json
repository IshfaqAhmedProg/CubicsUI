{
  "name": "@cubicsui/db",
  "version": "1.2.6",
  "description": "The database that holds all the constants required to generate components",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "keywords": [
    "studiocubics",
    "cubicsui",
    "db",
    "database"
  ],
  "author": {
    "name": "Studio Cubics",
    "email": "studiocubics7@gmail.com",
    "url": "https://studio-cubics.vercel.app"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rimraf node_modules dist generated",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev --skip-generate",
    "db:deploy": "prisma migrate deploy",
    "db:push": "prisma db push",
    "lint": "eslint",
    "build": "pnpm db:generate && pnpm lint && tsc"
  },
  "devDependencies": {
    "@cubicsui/configs": "workspace:^",
    "@types/node": "^22.12.0",
    "eslint": "^9.15.0",
    "prisma": "^6.3.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0"
  },
  "dependencies": {
    "@prisma/client": "^6.3.0"
  }
}
