{
  "name": "@cubicsui/gen",
  "version": "1.0.0",
  "description": "The main generator that builds the components, hooks and styles, based on what you need",
  "private": true,
  "main": "dist/main.js",
  "scripts": {
    "build:src": "tsc -p tsconfig.app.json",
    "build:gen": "tsc -p tsconfig.node.json",
    "build": "pnpm build:src && pnpm build:gen",
    "clean": "rimraf node_modules .turbo dist bin",
    "gen": "pnpm build && node bin/generator.js"
  },
  "keywords": [
    "gen",
    "studiocubics",
    "cubicsui",
    "cui"
  ],
  "author": {
    "name": "Studio Cubics",
    "email": "studiocubics7@gmail.com",
    "url": "https://studio-cubics.vercel.app"
  },
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.21",
    "commander": "^12.1.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.10.1",
    "typescript": "^5.7.2"
  }
}
