{
  "private": false,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "clean": "turbo clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "changeset": "changeset && changeset version",
    "release": "turbo build --filter=!./apps/* && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "eslint": "^9.15.0",
    "prettier": "^3.2.5",
    "turbo": "^2.3.3"
  },
  "engines": {
    "node": ">=18"
  },
  "name": "cubicsui",
  "packageManager": "pnpm@8.15.6",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
