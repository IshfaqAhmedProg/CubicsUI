{
  "private": false,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "clean": "turbo clean",
    "gen": "cd packages/gen/ && pnpm gen",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "changeset": "changeset && changeset version",
    "release": "turbo build --filter=@cubicsui/cli --filter=@cubicsui/gen && changeset publish",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "eslint": "^9.15.0",
    "prettier": "^3.4.1",
    "rimraf": "^6.0.1",
    "turbo": "^2.3.3"
  },
  "engines": {
    "node": ">=20.18.0"
  },
  "name": "cubicsui",
  "workspaces": [
    "apps/*",
    "packages/**",
    "jobs/*"
  ],
  "packageManager": "pnpm@9.14.4",
  "author": {
    "name": "Studio Cubics",
    "email": "studiocubics7@gmail.com",
    "url": "https://studio-cubics.vercel.app"
  }
}
